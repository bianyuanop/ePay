{
  "canisters": {
    "merchant": {
      "candid": "src/ePay_backend/src/canisters/merchant.did",
        "type": "custom",
        "wasm": "target/wasm32-unknown-unknown/release/merchant_opt.wasm",
        "build": [
          "cargo build --target wasm32-unknown-unknown --bin merchant --release",
          "ic-wasm target/wasm32-unknown-unknown/release/merchant.wasm -o target/wasm32-unknown-unknown/release/merchant_opt.wasm shrink"
        ] 
      },
    "dip20": {
      "candid": "src/ePay_backend/src/canisters/dip20.did",
        "type": "custom",
        "wasm": "target/wasm32-unknown-unknown/release/dip20_opt.wasm",
        "build": [
          "cargo build --target wasm32-unknown-unknown --bin dip20 --release",
          "ic-wasm target/wasm32-unknown-unknown/release/dip20.wasm -o target/wasm32-unknown-unknown/release/dip20_opt.wasm shrink"
        ] 
      },
    "manager": {
      "candid": "src/ePay_backend/src/canisters/manager.did",
        "type": "custom",
        "wasm": "target/wasm32-unknown-unknown/release/manager_opt.wasm",
        "build": [
          "cargo build --target wasm32-unknown-unknown --bin manager --release",
